#!/usr/bin/env python
# -*- coding: utf-8 -*-

import os
import sys
import argparse
import numpy as np


def main(arguments):
    parser = argparse.ArgumentParser()
    parser.add_argument("-i", '--salmon-quant-file', action='store', dest='quantfile', type=str, required=True)
    parser.add_argument("-o", action='store', dest='outfile', type=str, default=None)
    args = parser.parse_args(arguments)

if __name__ == '__main__':
    sys.exit(main(sys.argv[1:]))
